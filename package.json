{
  "name": "@layer-n/nord-ts",
  "version": "0.0.42",
  "description": "Typescript for Nord",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "nodemon --exec npx tsc",
    "build:proto": "./protoc-generate.sh && rm -f ./src/gen/action_pb.js",
    "test": "jest",
    "docs": "npx typedoc --out ./docs ./src",
    "build": "npm run build:proto && tsc && npm run docs",
    "clean": "rm -rf ./src/gen && rm -rf docs",
    "lint": "yarn eslint --fix --ext .ts --ext .js src",
    "fmt": "yarn prettier --write src",
    "ci": "yarn eslint --ext .ts --ext .js src && yarn prettier --check src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Layer-N/nord.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/google-protobuf": "^3.15.10",
    "@types/node": "^20.12.3",
    "@types/node-fetch": "^2.6.9",
    "@types/secp256k1": "^4.0.6",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@vercel/style-guide": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.7.0",
    "mocha": "^10.4.0",
    "prettier": "^3.1.1",
    "protoc-gen-js": "^3.21.2",
    "protoc-gen-ts": "^0.8.7",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.156.7",
    "typedoc": "^0.25.0",
    "typescript": "*"
  },
  "dependencies": {
    "@noble/curves": "^1.3.0",
    "@noble/hashes": "^1.3.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.23",
    "decimal.js": "^10.4.3",
    "ethers": "^6.11.1",
    "google-protobuf": "^3.21.2",
    "node-fetch": "2.6.13",
    "secp256k1": "^5.0.0",
    "ws": "^8.16.0"
  }
}
