{
  "name": "@n1xyz/nord-ts",
  "version": "0.0.4",
  "description": "Typescript for Nord",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "nodemon --exec npx tsc",
    "build:proto": "./protoc-generate.sh",
    "build:idl": "./idl-generate.sh",
    "build": "pnpm run build:proto && pnpm run build:idl && pnpm exec tsc && pnpm run docs",
    "test": "pnpm exec jest",
    "docs": "pnpm exec typedoc --out ./docs ./src",
    "compile": "tsc",
    "clean": "rm -rf ./src/gen ./dist ./docs",
    "fmt": "prettier --write src tests",
    "ci": "eslint && prettier --check src tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n1xyz/nord.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@types/google-protobuf": "^3.15.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.3",
    "@types/node-fetch": "^2.6.9",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.2.0",
    "typedoc": "^0.27.9",
    "typescript": "*"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.0.0",
    "@coral-xyz/anchor": "^0.30.1",
    "@noble/curves": "^1.3.0",
    "@noble/hashes": "^1.3.2",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.0",
    "bs58": "^6.0.0",
    "decimal.js": "^10.4.3",
    "ethers": "^6.11.1",
    "node-fetch": "2.6.13",
    "tweetnacl": "^1.0.3",
    "ws": "^8.16.0"
  }
}
